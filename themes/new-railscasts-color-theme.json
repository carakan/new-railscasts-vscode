{
  "name": "NewRailscasts",
  "type": "dark",
  "colors": {
    "editor.foreground": "#FEF9E1",
    "editor.background": "#202020",
    "activityBarBadge.background": "#007acc",
    "sideBarTitle.foreground": "#bbbbbb",
    "statusBar.background": "#252526",
    "editorBracketMatch.background": "#3344F09E",
    "editorBracketMatch.border": "#3344F09E",
    "editorRuler.foreground": "#FFFFFF24",
    "editor.lineHighlightBackground": "#202020",
    "editorSuggestWidget.background": "#000000D6",
    "editorHoverWidget.background": "#000000D6",
    "titleBar.activeBackground": "#0A0A0A",
    "titleBar.inactiveBackground": "#2C2C2C",
    "editorGroupHeader.tabsBackground": "#252526",
    "selection.background": "#072f95"
  },
  "tokenColors": [
    {
      "settings": {
        "foreground": "#FEF9E1",
        "background": "#000000",
        "lineHighlight": "#333435"
      }
    },
    {
      "name": "White",
      "scope": [
        "markup.underline",
        "meta.property-value constant",
        "meta.property-value support",
        "text.html.markdown meta.paragraph.markdown",
        "variable"
      ],
      "settings": {
        "foreground": "#FEF9E1"
      }
    },
    {
      "name": "Green Light",
      "scope": [
        "constant.numeric",
        "constant.numeric.css"
      ],
      "settings": {
        "foreground": "#bde04c"
      }
    },
    {
      "name": "Green",
      "scope": [
        "constant.numeric",
        "markup.inline.raw",
        "punctuation.definition.string.begin.elixir",
        "punctuation.definition.string.end.elixir",
        "string"
      ],
      "settings": {
        "foreground": "#74D155"
      }
    },
    {
      "name": "Orange",
      "scope": [
        "meta.class.ts",
        "constant.language",
        "entity.name.type.class.ruby",
        "keyword",
        "storage",
        "support.class.ruby",
        "text.html.markdown markup.heading punctuation",
        "text.html.markdown meta.paragraph punctuation",
        "variable.language.this",
        "variable.other.constant.elixir",
        "punctuation.definition.tag.haml"
      ],
      "settings": {
        "foreground": "#F47454"
      }
    },
    {
      "name": "Orange, italic",
      "scope": [
        "text.html.basic entity.other.attribute-name",
        "text.html.basic entity.other.attribute-name.html",
        "entity.other.attribute-name.class"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#F47454"
      }
    },
    {
      "name": "Orange, bold",
      "scope": [
        "keyword.control.class.ruby",
        "keyword.control.def.ruby",
        "keyword.control.module.elixir",
        "keyword.control.module.ruby",
        "storage.type.class.ts",
        "keyword.control.export.ts"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#F47454"
      }
    },
    {
      "name": "Yellow",
      "scope": [
        "entity.name.function",
        "entity.name.tag.css",
        "entity.other.attribute-name",
        "keyword.operator.other.elixir",
        "meta.function-call",
        "meta.selector entity",
        "meta.selector entity",
        "meta.tag support",
        "meta.tag",
        "markup.fenced_code.block.markdown",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.definition.tag.html",
        "support.function"
      ],
      "settings": {
        "foreground": "#FFC66D"
      }
    },
    {
      "name": "Yellow, bold-italic",
      "scope": [
        "meta.decorator.js punctuation.decorator.js",
        "meta.decorator.ts meta.function-call.ts entity.name.function.ts",
        "meta.decorator.ts punctuation.decorator.ts",
        "meta.definition.method.js entity.name.function.js",
        "meta.definition.method.ts entity.name.function.ts"
      ],
      "settings": {
        "fontStyle": "italic bold",
        "foreground": "#FFC66D"
      }
    },
    {
      "name": "White",
      "scope": [
        "meta.brace.round",
        "meta.jsx.children",
        "punctuation"
      ],
      "settings": {
        "foreground": "#FEF9E1"
      }
    },
    {
      "name": "Sky blue bold",
      "scope": [
        "constant.other.symbol.elixir punctuation.definition.constant.elixir",
        "constant.other.symbol.hashkey.ruby punctuation.definition.constant.ruby",
        "punctuation.definition.constant.ruby",
        "punctuation.separator.key-value.css",
        "punctuation.separator.key-value.js",
        "punctuation.separator.key-value.scss",
        "punctuation.separator.key-value.ts",
        "punctuation.support.type.property-name.begin.json.comments",
        "punctuation.support.type.property-name.end.json.comments"
      ],
      "settings": {
        "foreground": "#6D9CBE"
      }
    },
    {
      "name": "Sky blue bold",
      "scope": [
        "constant.language.symbol.hashkey.ruby",
        "constant.language.symbol.ruby",
        "constant.other.symbol.elixir punctuation.definition.constant.elixir",
        "constant.other.symbol.elixir",
        "constant.other.symbol.hashkey.ruby",
        "constant.other.symbol.ruby",
        "meta.object-literal.key.js",
        "meta.object-literal.key.ts",
        "support.type.property-name.css",
        "support.type.property-name.json",
        "meta.definition.property.ts variable.object.property.ts"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#6D9CBE"
      }
    },
    {
      "name": "Offwhite",
      "scope": [
        "constant",
        "punctuation.definition.block.tag.jsdoc",
        "storage.type.class.jsdoc",
        "support",
        "variable.other.property",
        "variable.other.readwrite.instance.ruby punctuation.definition.variable.ruby",
        "variable.other.readwrite.instance.ruby",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#D7B0FC"
      }
    },
    {
      "name": "Purple light italic",
      "scope": [
        "entity.other.attribute-name.class.css",
        "storage.type.js",
        "storage.type.ts",
        "support.type.primitive.ts"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#D7B0FC"
      }
    },
    {
      "name": "Purple light bold-italic",
      "scope": [
        "variable.other.readwrite.module.elixir punctuation.definition.variable.elixir",
        "variable.other.readwrite.module.elixir"
      ],
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#D7B0FC"
      }
    },
    {
      "name": "Brown, italic",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "punctuation.section.comment.haml"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#BC7F23"
      }
    },
    {
      "name": "orange-yellow, bold",
      "scope": [
        "punctuation.section.array.begin.ruby",
        "punctuation.section.array.elixir",
        "punctuation.section.array.end.ruby",
        "punctuation.section.scope.begin.ruby",
        "punctuation.section.scope.elixir",
        "punctuation.section.scope.end.ruby",
        "punctuation.separator.method.ruby"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#D38E39"
      }
    }
  ]
}
