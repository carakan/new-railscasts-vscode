{
  "name": "NewRailscasts",
  "type": "dark",
  "colors": {
    "editor.foreground": "#FEF9E1",
    "editor.background": "#202020",
    "activityBarBadge.background": "#007acc",
    "sideBarTitle.foreground": "#bbbbbb",
    "statusBar.background": "#252526",
    "editorBracketMatch.background": "#3344F09E",
    "editorBracketMatch.border": "#3344F09E",
    "editorRuler.foreground": "#FFFFFF24",
    "editor.lineHighlightBackground": "#202020",
    "editorSuggestWidget.background": "#000000D6",
    "editorHoverWidget.background": "#000000D6",
    "titleBar.activeBackground": "#0A0A0A",
    "titleBar.inactiveBackground": "#2C2C2C",
    "editorGroupHeader.tabsBackground": "#252526"
  },
  "tokenColors": [
    {
      "settings": {
        "foreground": "#FEF9E1",
        "background": "#000000",
        "lineHighlight": "#333435"
      }
    },
    {
      "name": "Cyan",
      "scope": [
        "variable",
        // Markdown: link
        "markup.underline",
        // CSS property value
        "meta.property-value support",
        "meta.property-value constant"
      ],
      "settings": {
        "foreground": "#FEF9E1"
      }
    },
    {
      "name": "Green Light",
      "scope": [
        // JavaScript: numbers
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#bde04c"
      }
    },
    {
      "name": "Green",
      "scope": [
        "string",
        // JavaScript: numbers
        "constant.numeric",
        // Markdown: stuff between tick marks, i.e. `stuff`
        "markup.inline.raw",
        // Elixir
        "punctuation.definition.string.begin.elixir",
        "punctuation.definition.string.end.elixir"
      ],
      "settings": {
        "foreground": "#74D155"
      }
    },
    {
      "name": "Orange",
      "scope": [
        // JavaScript: return, export, yield
        "keyword",
        // JavaScript: let, var, const
        "storage",
        // JavaScript: true, false
        "constant.language",
        // JavaScript: this
        "variable.language.this",
        // Markdown: punctuation, such as ##, **, _, etc...
        "text.html.markdown meta.paragraph punctuation",
        "text.html.markdown markup.heading punctuation",
        // Elixir
        "variable.other.constant.elixir",
        // Ruby
        "support.class.ruby"
      ],
      "settings": {
        "foreground": "#F47454"
      }
    },
    {
      "name": "Orange, italic",
      "scope": [
        // HTML
        "entity.other.attribute-name.html",
        "entity.other.attribute-name.html entity.other.attribute-name.generic.html"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#F47454"
      }
    },
    {
      "name": "Orange, bold",
      "scope": [
        // Ruby
        "keyword.control.class.ruby",
        "keyword.control.def.ruby",
        // Elixir
        "keyword.control.module.elixir"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#F47454"
      }
    },
    {
      "name": "White and underlined",
      "scope": [
        "variable.parameter"
      ],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#FEF9E1"
      }
    },
    {
      "name": "Yellow",
      "scope": [
        // HTML: tag
        "meta.tag",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        // JSX/TSX: classes
        "meta.tag support",
        // JavaScript: function call
        "meta.function-call",
        // JavaScript: function declarations/expressions
        "entity.name.function",
        // JavaScript: built-in functions
        "support.function",
        // CSS selector
        "meta.selector entity",
        "meta.selector entity",
        "entity.name.tag.css",
        "entity.other.attribute-name",
        // Elixir
        "keyword.operator.other.elixir"
      ],
      "settings": {
        "foreground": "#FFC66D"
      }
    },
    {
      "name": "Yellow, bold-italic",
      "scope": [
        "meta.definition.method.js entity.name.function.js",
        "meta.definition.method.ts entity.name.function.ts"
      ],
      "settings": {
        "fontStyle": "italic bold",
        "foreground": "#FFC66D"
      }
    },
    {
      "name": "White",
      "scope": [
        "meta.brace.round",
        "meta.jsx.children",
        "punctuation"
      ],
      "settings": {
        "foreground": "#FEF9E1"
      }
    },
    {
      "name": "Sky blue bold",
      "scope": [
        //JSON
        "support.type.property-name.json",
        "meta.object-literal.key.js",
        // Ruby
        "constant.language.symbol.ruby",
        "punctuation.definition.constant.ruby",
        // Elixir
        "constant.other.symbol.elixir",
        // CSS
        "punctuation.separator.key-value.css",
        "support.type.property-name.css"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#6D9CBE"
      }
    },
    {
      "name": "Offwhite",
      "scope": [
        "meta",
        // JavaScript: object properties
        "variable.other.property",
        "constant",
        // JavaScript: built-in methods
        "support",
        // JavaScript: jsDoc @param
        "punctuation.definition.block.tag.jsdoc",
        "storage.type.class.jsdoc",
        // RUBY
        "variable.other.readwrite.instance.ruby"
      ],
      "settings": {
        "foreground": "#D7B0FC"
      }
    },
    {
      "name": "Purple light italic",
      "scope": [
        // CSS
        "entity.other.attribute-name.class.css"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#D7B0FC"
      }
    },
    {
      "name": "Purple light bold-italic",
      "scope": [
        "meta.decorator.ts",
        // Elixir
        "variable.other.readwrite.module.elixir",
        "variable.other.readwrite.module.elixir punctuation.definition.variable.elixir"
      ],
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#D7B0FC"
      }
    },
    {
      "name": "Brown, italic",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#BC7F23"
      }
    },
    {
      "name": "orange-yellow, bold",
      "scope": [
        "punctuation.separator.method.ruby",
        "punctuation.section.array.begin.ruby",
        "punctuation.section.array.end.ruby",
        "punctuation.section.scope.begin.ruby",
        "punctuation.section.scope.end.ruby",
        // Elixir
        "punctuation.section.array.elixir",
        "punctuation.section.scope.elixir"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#D38E39"
      }
    }
  ]
}
